{
  "name": "@monkeytype/shared-types",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "dev": "tsc --watch ",
    "build": "tsc --build --clean",
    "check-ts": "tsc --noEmit",
    "lint": "eslint \"./**/*.ts\""
  },
  "devDependencies": {
    "typescript": "5.5.3",
    "@monkeytype/typescript-config": "*"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.ts"
    },
    "./*": {
      "default": "./dist/*.js",
      "types": "./dist/*.ts"
    }
  }
}
